    <div class="panel panel-primary">
        <div class="panel-heading">
            <span class="panel-title">{{fromtitle}}</span>

        </div>
        <form class="form-horizontal" name="cdpform">
            <div class="panel-body">
                <h2 style="margin-top: 0;" class="page-header">Career Developement Plan</h2>





                <div class="form-group">
                    <label class="col-lg-2 control-label">Employee ID:</label>
                    <div class="col-lg-3">

                        <input type="text" class="form-control" disabled id="employeeId" ng-model="employeeId">

                    </div>


                </div>


                <div class="form-group">
                    <label class="col-lg-2 control-label">Name:</label>
                    <div class="col-lg-3" ng-show="showEmployeeNameDropdown">
                        <selectize placeholder='--Select Employee--' options='employeeOptions' config="{maxItems: 1}" ng-model="employeeId" ng-change="ChangeEmployeeName()" required>  </selectize>
                    </div>
                    <div class="col-lg-3" ng-show="showEmployeeName">
                        <input type="text" disabled class="form-control" ng-model="employename">
                    </div>

                </div>
                <div class="form-group">
                    <label class="col-lg-2 control-label">Year:</label>
                    <div class="col-lg-3" ng-show="showYearDropdown">
                        <selectize placeholder='--Select Year--' options='yearOptions' config="{maxItems: 1}" ng-model="year" ng-change="ChangeinYearCovered();" ng-disabled="addCdpyear" required>  </selectize>
                        <div class="alert-danger" ng-show="hasCdp">
                            <strong>You Have Already CDP for the year {{year}}</strong>
                        </div>
                    </div>
                    <div class="col-lg-3" ng-show="showYear">
                        <input type="text" disabled class="form-control" ng-model="year">
                    </div>
                </div>

                <button class="btn btn-success btn-gradient btn-rounded pull-right" ng-disabled="cdpform.$invalid ||  hasCdp" ng-click="SaveCDP();" ng-hide="addCdpyear">Save</button>

            </div>




            <div class="panel-body" ng-show="addCdpyear">
                <div style="margin-bottom:5px;">
                    <a class="btn btn-primary btn-sm btn-gradient btn-rounded pointer" ng-click="OpenAddGoalModal()">Add Goal Plan</a>
                    <h2 ng-hide="category.length <= 0">Category</h2>
                </div>




                <accordion class="row margin">
                    <accordion-group ng-repeat="categories in category | groupBy:'categoryname'" class="panel-category">
                        <accordion-heading style="text-decoration:none;" ng-repeat="categoryList in categories  | unique:'categoryname'">
                            {{$parent.$index + 1}}.
                            &nbsp; {{categoryList.categoryname}}
                        </accordion-heading>
                        <div class="accordion-inner" overflow-y scroll; overflow-x hidden;">
                            <div class="row margin">
                                <div class="col-lg-2">
                                    <h2>Goal Plan</h2>

                                </div>
                            </div>
                            <accordion class="row margin" style="background-color:#3333FF;">

                                <accordion-group ng-repeat="goalplanList in categories track by $index" class="panel-goal">
                                    <accordion-heading style="text-decoration:none;">
                                        {{$index + 1}}.
                                        &nbsp; {{goalplanList.goaldescription}}
                                        <div class="pull-right">

                                            <button class="btn btn-success btn-gradient btn-rounded" ng-click="openModalForm('#modal-panel-editgoalplan');$event.stopPropagation();GetGoalPlan(goalplanList);">
                                                <i class="glyphicon glyphicon-pencil "></i>
                                                Edit
                                            </button>
                                            <button class="btn btn-danger btn-gradient btn-rounded" ng-click="openModalForm('#modal-panel-deletegoalplan');$event.stopPropagation();GetGoalPlan(goalplanList);">
                                                <i class="glyphicon glyphicon-trash"></i>
                                                Delete
                                            </button>
                                        </div>

                                    </accordion-heading>
                                    <div class="accordion-inner" overflow-y scroll; overflow-x hidden;">

                                        <div class="row margin">
                                            <div class="col-lg-2">
                                                <h2 ng-hide="goalplanList.actionplan.length <=0">Action Plan</h2>
                                                <a class="btn btn-info btn-sm btn-gradient btn-rounded pointer" ng-click="openModalForm('#modal-panel-addactionplan');GetGoalPlan(goalplanList);">Add Action Plan</a>

                                            </div>
                                        </div>

                                        <!--Action Plan List-->
                                        <table class="table table-striped table-condensed" ng-hide="goalplanList.actionplan <= 0">
                                            <tr>
                                                <th>Action</th>
                                                <th>Action Plan</th>
                                                <th>Target Completion Date</th>
                                                <th>Actual Completion Date</th>
                                                <th>Status</th>
                                                <th>Remarks</th>
                                                <th>Updated Date</th>
                                               
                                            </tr>
                                            <tr ng-repeat="actionplanList in goalplanList.actionplan">
                                                <td>





                                                    <button class="btn btn-success btn-gradient btn-rounded" ng-click="openModalForm('#modal-panel-editactionplan');GetActionPlan(actionplanList);" ng-hide="actionplanList.iscompleted === 1">
                                                        <i class="glyphicon glyphicon-pencil "></i>
                                                        Edit
                                                    </button>
                                                    <button class="btn btn-danger btn-gradient btn-rounded" ng-click="openModalForm('#modal-panel-deleteactionplan');GetActionPlan(actionplanList);" ng-hide="actionplanList.iscompleted === 1">
                                                        <i class="glyphicon glyphicon-trash"></i>
                                                        Delete
                                                    </button>
                                                 


                                                </td>

                                                <td>{{actionplanList.actiondescription}}</td>
                                                <td>{{actionplanList.targetdate | date:'mediumDate'}}</td>
                                                <td>{{actionplanList.completedate === null ? 'Not yet Completed':actionplanList.completedate| date:'mediumDate'}}</td>
                                                <td>{{actionplanList.iscompleted === 0 ? 'Open':'Close'}}</td>
                                                <td>
                                                    <table>
                                                        <tr ng-repeat="actionplanlistcomment in actionplanList.actioncomment" ng-if="actionplanList.actioncomment.length>0">
                                                            <td style="white-space:nowrap">
                                                                {{actionplanlistcomment.actioncomment}}
                                                            </td>

                                                        </tr>
                                                        <tr ng-if="actionplanList.actioncomment == null || actionplanList.actioncomment.length === 0">
                                                            <td>
                                                                No Remarks
                                                            </td>

                                                        </tr>
                                                    </table>




                                                </td>
                                                <td>
                                                    <table>
                                                        <tr ng-repeat="actionplanlistcomment in actionplanList.actioncomment" ng-if="actionplanList.actioncomment.length>0">
                                                            <td style="white-space:nowrap">
                                                                {{actionplanlistcomment.createdate | date:'mediumDate'}}
                                                            </td>

                                                        </tr>
                                                        <tr ng-if="actionplanList.actioncomment == null || actionplanList.actioncomment.length === 0">
                                                            <td>
                                                                No Update Yet
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </td>
                                                <td>


                                              
                                                </td>
                                            </tr>
                                        </table>
                                        <!--Show this if the Action plan list is empty-->
                                        <H1 ng-show="goalplanList.actionplan.length <= 0">NO data!</H1>
                                    </div>
                                </accordion-group>
                                <H1 ng-show="categories.length <= 0">NO data!</H1>
                            </accordion>
                        </div>
                    </accordion-group>
                    <H1 ng-show="category.length <= 0">NO data!</H1>
                </accordion>


            </div>
            <div class="panel-footer text-right" ng-show="addCdpyear">


                <button class="btn btn-success btn-gradient btn-rounded" ng-click="BacktoCDP();">Done</button>

            </div>
        </form>
    
    </div>

</section>




