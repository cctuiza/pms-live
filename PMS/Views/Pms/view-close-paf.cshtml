
@{
    Layout = null;
}



<section class="content ">
    <div class="panel panel-solid">
        <div class="panel-body" data-ng-init="onInit()">
            <h2 class="page-header">PAF List(Employee List)</h2>
       

            <div data-ng-repeat="x in pafMaster" data-ng-show="onViewDetails(x.pmsEmployeeDtl)">
                <h3>{{onType(x.pmsLstType)}}</h3>
                <table class="table table-condensed table-hover" ng-if="x.pmsLstType==1">
                    <tr>
                        <th></th>
                        <th>Employee ID</th>
                        <th>Employee Name</th>
                        <th>Department</th>
                        <th>Position</th>
                        <th>Immediate Head</th>

                    </tr>
                    <tr data-ng-repeat="xx in x.pmsEmployeeDtl">
                        <td><a class="pointer" ng-show="onCheckClosePaf(xx.pmsPafMaster)" data-ng-click="onViewClosePaf(xx,x.pmsLstType)">View List</a></td>
                        <td>{{xx.empId}}</td>
                        <td>{{xx.empName}}</td>
                        <td>{{xx.department}}</td>
                        <td>{{xx.position}}</td>
                        <td>{{xx.immediateHead}}</td>
                    </tr>
                </table>
                <!-- pmsLstType=2-->
                <table class="table table-condensed table-hover" ng-if="x.pmsLstType==2">
                    <tr>
                        <th></th>
                        <th>Employee ID</th>
                        <th>Employee Name</th>
                        <th>Department</th>
                        <th>Position</th>
                        <th>Immediate Head</th>
                    </tr>
                    <tr data-ng-repeat="xx in x.pmsEmployeeDtl  | filter:search:strict">
                        <td><a class="pointer" ng-show="onCheckClosePaf(xx.pmsPafMaster)" data-ng-click="onViewClosePaf(xx,x.pmsLstType)">View List</a></td>
                        <td>{{xx.empId}}</td>
                        <td>{{xx.empName}}</td>
                        <td>{{xx.department}}</td>
                        <td>{{xx.position}}</td>
                        <td>{{xx.immediateHead}}</td>
                    </tr>
                </table>
                <!-- pmsLstType=3-->
                <table class="table table-condensed table-hover" ng-if="x.pmsLstType==3">
                    <tr>
                        <th></th>
                        <th>Employee ID</th>
                        <th>Employee Name</th>
                        <th>Department</th>
                        <th>Position</th>
                        <th>Immediate Head</th>
                    </tr>
                    <tr data-ng-repeat="xx in x.pmsEmployeeDtl  | filter:search:strict">
                        <td><a class="pointer" ng-show="onCheckClosePaf(xx.pmsPafMaster)" data-ng-click="onViewClosePaf(xx,x.pmsLstType)">View List</a></td>
                        <td>{{xx.empId}}</td>
                        <td>{{xx.empName}}</td>
                        <td>{{xx.department}}</td>
                        <td>{{xx.position}}</td>
                        <td>{{xx.immediateHead}}</td>
                    </tr>
                </table>
            </div>


        </div>
    </div>
</section>


<!-- modal view for applicant remove prompt-->
<div id="modal-panel-paflist" class="popup-basic  popup-lg bg-none mfp-with-anim mfp-hide">
    <div class="panel">
        <div class="panel-heading">
            <span class="panel-title"><i class="glyphicon glyphicon-tasks"></i>PAF List</span>
        </div>
        <div class="panel-body">
            <p class="text-danger">{{stopnote}}</p>
            <table class="table table-condensed table-hover">
                <tr>
                    <th></th>
                    <th>Covered</th>
                    <th>Created Date</th>
                    <th>Status</th>
                </tr>
                <tr data-ng-repeat="x in panoList" ng-if="x.Status=='C'">
                    <td>
                        <button class="btn btn-primary btn-xs" data-ng-click="onViewClosePAFHistory(x.hashcode)">view</button>

                    </td>
                    <td>{{x.COVERED_FROM | date:'MM/dd/yyyy'}}&nbsp;to&nbsp;{{x.COVERED_TO | date:'MM/dd/yyyy'}}</td>
                    <td>{{x.DT_OF_AP | date:'MM/dd/yyyy'}}</td>
                    <td>{{x.Status=='C' ? 'Close':'Open'}}</td>
                </tr>
            </table>
        </div>
        <div class="panel-footer text-right">
            <button class="btn" data-ng-click="closeModalForm()">Cancel</button>
        </div>
    </div>
</div>


